{
    "name": "pandaleague/mock-server",
    "description": "Mock a server connection (HTTP, STOMP)",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Edmund le Roux",
            "email": "edmund.leroux@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "PandaLeague\\MockServer\\": "src/"
        }
    },
    "require": {
        "stomp-php/stomp-php": "^5.0",
        "cboden/ratchet": "^0.4.3",
        "symfony/process": "^5.1",
        "symfony/console": "^5.1",
        "doctrine/dbal": "^2.11||^3.0",
        "ramsey/uuid": "^3.9",
        "php": "^7.4||^8.0"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.2",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
